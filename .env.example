# Project Configuration
PROJECT_PREFIX=rimac-challenge
STAGE=prod

# AWS Configuration
AWS_PROFILE=your-aws-profile-name
AWS_REGION=us-east-1

# Alternative AWS Configuration (if not using profile)
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key

# Application
NODE_ENV=production
PORT=3000

# Service Names (will be prefixed with PROJECT_PREFIX-STAGE)
APPOINTMENT_SERVICE_NAME=appointment
APPOINTMENT_PE_SERVICE_NAME=appointment-pe
APPOINTMENT_CL_SERVICE_NAME=appointment-cl

# Variables para conexi√≥n a RDS (MySQL)
RDS_HOST=your-rds-host.amazonaws.com
RDS_PORT=3306
RDS_USERNAME=youruser
RDS_PASSWORD=yourpassword
RDS_DATABASE=your_database

# Database (example for DynamoDB or other services)
# DATABASE_URL=your_database_url

# API Keys
# EXTERNAL_API_KEY=your_api_key
